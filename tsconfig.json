{
  "baseUrl": ".",
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "moduleResolution": "node16",
    "module": "node16",
  },
  "paths": {
    "@/*": [
      "src/*"
    ]
  },
}